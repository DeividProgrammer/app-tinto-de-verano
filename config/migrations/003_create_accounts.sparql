PREFIX mu:      <http://mu.semte.ch/vocabularies/core/>
PREFIX foaf:    <http://xmlns.com/foaf/0.1/>
PREFIX account: <http://mu.semte.ch/vocabularies/account/>
PREFIX dct:     <http://purl.org/dc/terms/>

# Crear cuentas de usuario para login/registration services
# Los servicios mu-login y mu-registration esperan este modelo de datos

INSERT DATA {
  GRAPH <http://mu.semte.ch/graphs/public> {
    # Account para Alice
    <http://tinto.app/accounts/account-1>
      a foaf:OnlineAccount ;
      mu:uuid "account-1" ;
      foaf:accountServiceHomepage <http://tinto.app> ;
      foaf:accountName "alice@example.com" .

    # Account para Bob
    <http://tinto.app/accounts/account-2>
      a foaf:OnlineAccount ;
      mu:uuid "account-2" ;
      foaf:accountServiceHomepage <http://tinto.app> ;
      foaf:accountName "bob@example.com" .
  }

  GRAPH <http://mu.semte.ch/graphs/users> {
    # Relación Account → User para Alice
    <http://tinto.app/accounts/account-1> foaf:account <http://tinto.app/users/user-1> .

    # Relación Account → User para Bob
    <http://tinto.app/accounts/account-2> foaf:account <http://tinto.app/users/user-2> .
  }
}

